<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.PbrandMapper">
   <select id="getById" parameterType="int" resultType="Pbrand">
      select * from pbrand where pbid=#{pbid}
   </select>
   <select id="listAll" resultType="Pbrand">
      select * from pbrand
   </select>
   
</mapper>